{
  "author": "ns8inc",
  "dependencies": {
    "typescript": "3.7.4"
  },
  "description": "NS8 Protect JavaScript SDK",
  "devDependencies": {
    "@ns8/eslint-config-ns8-typescript-eslint": "0.0.7",
    "@ns8/ns8-protect-sdk": "0.1.24",
    "@types/chai": "4.2.7",
    "@types/mocha": "5.2.7",
    "@types/node": "13.1.1",
    "@types/semver": "6.2.0",
    "@typescript-eslint/eslint-plugin": "2.13.0",
    "@typescript-eslint/parser": "2.13.0",
    "awesome-typescript-loader": "5.2.1",
    "chai": "4.2.0",
    "create-ts-index": "1.12.0",
    "dotenv-extended": "2.7.1",
    "dts-bundle": "0.7.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.2",
    "hard-source-webpack-plugin": "0.13.1",
    "husky": "3.1.0",
    "mocha": "6.2.2",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "semver": "7.1.1",
    "source-map-support": "0.5.16",
    "ts-mocha": "6.0.0",
    "ts-node": "8.5.4",
    "typedoc": "0.15.6",
    "typedoc-plugin-markdown": "2.2.14",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ns8inc/protect-js-sdk#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "name": "protect-js-sdk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ns8inc/protect-js-sdk.git"
  },
  "scripts": {
    "beautify": "ts-node ./build/sortJson.ts",
    "build": "rm -rf dist && tsc -d --outDir dist && yarn bundle",
    "build:dev": "rm -rf dist && tsc --sourceMap && yarn bundle",
    "bundle": "yarn beautify && yarn generate:exports && webpack",
    "clean": "rm -rf .awcache .tmp dist",
    "docs": "rm -rf docs && yarn typedoc --plugin typedoc-plugin-markdown && docs:sync-build-scripts",
    "docs:sync-build-scripts": "ts-node src/scriptsDocumentation.ts",
    "generate:exports": "yarn cti create ./src -w",
    "lint": "yarn eslint src/**/*.ts test/**/*.ts",
    "lint-fix": "yarn eslint --fix src/**/*.ts test/**/*.ts",
    "test": "nyc --reporter=html --reporter=text mocha test/**/*Test.ts"
  },
  "scriptsDocumentation": {
    "beautify": {
      "description": "Perform aesthetic operations to make the project files easier to navigate and read.",
      "dev": true
    },
    "build": {
      "description": "Builds and bundles the project.",
      "dev": true
    },
    "build:dev": {
      "description": "Build the project with source maps for easier development",
      "dev": true
    },
    "bundle": {
      "description": "Bundles the project.",
      "dev": true
    },
    "clean": {
      "description": "Removes build artifacts.",
      "dev": true
    },
    "docs": {
      "description": "Generates documentation from typescript code.",
      "dev": true
    },
    "generate:exports": {
      "description": "Exports SDK functions to a top-level index.ts file in the src directory.",
      "dev": true
    },
    "lint": {
      "description": "Lints the codebase.",
      "dev": true
    },
    "test": {
      "description": "Runs tests and calculates test coverage.",
      "dev": true
    }
  },
  "version": "0.0.2"
}
