{
  "author": "ns8inc",
  "dependencies": {
    "typescript": "3.7.4"
  },
  "description": "NS8 Protect JavaScript SDK",
  "devDependencies": {
    "@ns8/eslint-config-ns8-typescript-eslint": "0.0.7",
    "@ns8/ns8-protect-sdk": "0.1.24",
    "@types/chai": "4.2.7",
    "@types/mocha": "5.2.7",
    "@types/node": "13.1.1",
    "@types/semver": "6.2.0",
    "@typescript-eslint/eslint-plugin": "2.13.0",
    "@typescript-eslint/parser": "2.13.0",
    "awesome-typescript-loader": "5.2.1",
    "chai": "4.2.0",
    "create-ts-index": "1.12.0",
    "dotenv-extended": "2.7.0",
    "dts-bundle": "0.7.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.2",
    "hard-source-webpack-plugin": "0.13.1",
    "husky": "3.1.0",
    "mocha": "6.2.2",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "semver": "7.1.1",
    "source-map-support": "0.5.16",
    "ts-mocha": "6.0.0",
    "ts-node": "8.5.4",
    "webpack": "4.41.4",
    "webpack-cli": "3.3.10"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ns8inc/protect-js-sdk#readme",
  "husky": {
    "hooks": {
      "pre-push": "yarn build && yarn lint && yarn test"
    }
  },
  "license": "MIT",
  "main": ".eslintrc.js",
  "name": "protect-js-sdk",
  "nyc": {
    "branches": 100,
    "checkCoverage": true,
    "exclude": [],
    "extension": [
      ".ts"
    ],
    "functions": 100,
    "instrument": true,
    "lines": 100,
    "reporter": [
      "text-summary",
      "html"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "sourceMap": true,
    "statements": 100
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ns8inc/protect-js-sdk.git"
  },
  "scripts": {
    "beautify": "ts-node ./build/sortJson.ts",
    "build": "rm -rf dist && tsc -d && yarn bundle",
    "build:dev": "rm -rf dist && tsc --sourceMap && yarn bundle",
    "bundle": "yarn beautify && yarn generate:exports && webpack",
    "clean": "rm -rf .awcache .tmp dist",
    "generate:exports": "yarn cti create ./src -w",
    "lint": "yarn eslint src/**/*.ts test/**/*.ts",
    "test": "nyc --reporter=html --reporter=text mocha test/**/*Test.ts"
  },
  "version": "0.0.1"
}
