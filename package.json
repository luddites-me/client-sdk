{
  "author": "NS8.com",
  "bugs": {
    "url": "https://github.com/ns8inc/protect-js-sdk/issues"
  },
  "description": "NS8 Protect SDK",
  "devDependencies": {
    "@ns8/eslint-config-ns8-typescript-eslint": "^0.0.7",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "chai": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "source-map-support": "^0.5.16",
    "ts-mocha": "^6.0.0",
    "typescript": "^3.7.3"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ns8inc/protect-js-sdk#readme",
  "husky": {
    "hooks": {
      "pre-push": "yarn build && yarn lint"
    }
  },
  "license": "MIT",
  "main": ".eslintrc.js",
  "name": "protect-js-sdk",
  "nyc": {
    "checkCoverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "exclude": [],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ns8inc/protect-js-sdk.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -d --sourceMap",
    "lint": "yarn eslint src/**/*.ts test/**/*.ts",
    "test": "nyc --reporter=html --reporter=text mocha test/**/*Test.ts"
  },
  "version": "1.0.0"
}
