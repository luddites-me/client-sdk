{
  "author": "ns8inc",
  "dependencies": {
    "@ns8/protect-tools-bundle": "0.0.13",
    "@ns8/protect-tools-js": "0.0.10",
    "cross-fetch": "3.0.4",
    "format-util": "1.0.5",
    "iframe-resizer": "4.2.10",
    "loglevel": "1.6.7",
    "stacktrace-js": "2.0.2",
    "typescript": "3.8.3",
    "webpack-cli": "3.3.11"
  },
  "description": "NS8 Protect JavaScript SDK",
  "devDependencies": {
    "@ns8/protect-tools-lint": "0.0.4",
    "@types/chai": "4.2.11",
    "@types/chai-as-promised": "7.1.2",
    "@types/format-util": "1.0.1",
    "@types/glob": "^7.1.1",
    "@types/jsdom": "16.2.0",
    "@types/mocha": "7.0.2",
    "@types/node": "13.11.0",
    "@types/node-fetch": "2.5.5",
    "@types/semver": "7.1.0",
    "@types/uuid-validate": "0.0.1",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "create-ts-index": "1.12.1",
    "dotenv-extended": "2.8.0",
    "husky": "4.2.3",
    "jsdom": "16.2.2",
    "mocha": "7.1.1",
    "nock": "12.0.3",
    "node-fetch": "2.6.0",
    "nyc": "15.0.1",
    "semver": "7.2.1",
    "sorted-json": "0.2.6",
    "ts-mocha": "7.0.0",
    "ts-node": "8.8.2",
    "typedoc": "0.17.4",
    "typedoc-plugin-markdown": "2.2.17",
    "uuid-validate": "0.0.3"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ns8inc/protect-sdk-client#readme",
  "license": "MIT",
  "main": "dist/protect.min.js",
  "name": "@ns8/protect-sdk-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ns8inc/protect-sdk-client.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --emitDeclarationOnly && yarn bundle",
    "build:prod": "NODE_ENV=production yarn build",
    "bundle": "yarn sortJson && yarn generate:exports && webpack && yarn docs:standardize",
    "clean": "rm -rf .awcache dist",
    "docs": "rm -rf docs && yarn typedoc --plugin typedoc-plugin-markdown && yarn docs:sync-build-scripts",
    "docs:standardize": "npx ./node_modules/@ns8/protect-tools-js/dist/readme/standardizeReadme.js",
    "generate:exports": "yarn cti create ./src -w",
    "lint": "yarn eslint -c 'node_modules/@ns8/protect-tools-lint/eslintrc.yml' 'src/**/*.ts'",
    "lint:fix": "yarn eslint -c 'node_modules/@ns8/protect-tools-lint/eslintrc.yml' --fix 'src/**/*.ts'",
    "sortJson": "npx ./node_modules/@ns8/protect-tools-js/dist/sortJson/sortJson.js",
    "test": "yarn build && nyc --nycrc-path 'node_modules/@ns8/protect-tools-lint/nycrc.yml' mocha 'src/**/*.test.ts'",
    "test:coverage": "nyc --nycrc-path 'node_modules/@ns8/protect-tools-lint/nycrc.yml' yarn test:debug",
    "test:debug": "mocha -r ts-node/register 'src/**/*.test.ts'"
  },
  "scriptsDocumentation": {
    "build": {
      "description": "Builds and bundles the project.",
      "dev": true
    },
    "build:dev": {
      "description": "Build the project with source maps for easier development.",
      "dev": true
    },
    "build:prod": {
      "description": "Builds and minifies the project for distribution.",
      "dev": false
    },
    "bundle": {
      "description": "Bundles the project.",
      "dev": true
    },
    "clean": {
      "description": "Removes build artifacts.",
      "dev": true
    },
    "docs": {
      "description": "Generates documentation from typescript code.",
      "dev": true
    },
    "docs:standardize": {
      "description": "Updates the project's documentation according to the markdown standard.",
      "dev": true
    },
    "generate:exports": {
      "description": "Exports SDK functions to a top-level index.ts file in the src directory.",
      "dev": true
    },
    "lint": {
      "description": "Lints the codebase.",
      "dev": true
    },
    "sortJson": {
      "description": "Perform aesthetic operations to make the project files easier to navigate and read.",
      "dev": true
    },
    "test": {
      "description": "Runs tests and calculates test coverage.",
      "dev": true
    }
  },
  "types": "dist/types/index.d.ts",
  "version": "0.0.130"
}
